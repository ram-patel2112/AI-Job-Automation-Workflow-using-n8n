    {
      "parameters": {
        "html": "=<!DOCTYPE html><html><head><style>body{font-family:Arial,sans-serif;margin:40px;line-height:1.6}.header{text-align:center;margin-bottom:30px}.header h1{margin:0;font-size:28px}.contact{text-align:center;margin-bottom:20px}.section{margin-bottom:25px}.section h2{border-bottom:2px solid #333;padding-bottom:5px;margin-bottom:15px}.job,.edu{margin-bottom:15px}.job h3,.edu h3{margin:0;font-size:16px}.job .duration,.edu .year{color:#666;font-style:italic}.skills{display:flex;flex-wrap:wrap;gap:10px}.skill{background:#f0f0f0;padding:5px 10px;border-radius:3px}</style></head><body><div class=\"header\"><h1>={{ $json.fullName }}</h1></div><div class=\"contact\"><p>={{ $json.email }} | ={{ $json.phone }}</p></div><div class=\"section\"><h2>Professional Summary</h2><p>Experienced professional seeking ={{ $json.targetJob }} position</p></div><div class=\"section\"><h2>Skills</h2><div class=\"skills\">={{ $json.skills.split(',').map(s => '<span class=\"skill\">' + s.trim() + '</span>').join('') }}</div></div><div class=\"section\"><h2>Work Experience</h2>={{ $json.experience.map(exp => '<div class=\"job\"><h3>' + exp.position + ' at ' + exp.company + '</h3><p class=\"duration\">' + exp.duration + '</p><p>' + exp.description + '</p></div>').join('') }}</div><div class=\"section\"><h2>Education</h2>={{ $json.education.map(edu => '<div class=\"edu\"><h3>' + edu.degree + '</h3><p>' + edu.institution + '</p><p class=\"year\">' + edu.year + '</p></div>').join('') }}</div></body></html>"
      },
