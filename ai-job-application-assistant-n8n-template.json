      "parameters": {
        "promptType": "define",
        "text": "=You are a professional resume writer.\n\nUsing the following details, generate a clean and professional resume content.\n\nName: {{$json.name}}\nSkills: {{$json.skills}}\nExperience: {{$json.experience}}\nEducation: {{$json.education}}\nLocation: {{$json.location}}\n\nFormat it properly.\n",
        "hasOutputParser": true,
        "options": {
          "systemMessage": "You are a helpful AI job application assistant. Your role is to: 1) Collect user profile information (skills, work experience, education, contact details), 2) Search for relevant job opportunities based on their profile, 3) Generate ATS-friendly resumes tailored to specific jobs, 4) Help users apply to jobs. Always be professional, encouraging, and guide users through each step. Ask clarifying questions when needed."
        }
      },
      "id": "ecf14f77-3d29-419d-948d-676c77a0e851",
